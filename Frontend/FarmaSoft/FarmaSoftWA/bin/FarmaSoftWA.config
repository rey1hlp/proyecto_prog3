<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.8.1" />
    <httpRuntime targetFramework="4.8.1" />
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="FarmaciaAsociadaWSPortBinding" />
        <binding name="MedicinaPropiaWSPortBinding" />
        <binding name="DetalleSolicitudWSPortBinding" />
        <binding name="AdministradorWSPortBinding" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8080/FarmaSoftWSBL/FarmaciaAsociadaWS"
        binding="basicHttpBinding" bindingConfiguration="FarmaciaAsociadaWSPortBinding"
        contract="ServicioWS.FarmaciaAsociadaWS" name="FarmaciaAsociadaWSPort" />
      <endpoint address="http://localhost:8080/FarmaSoftWSBL/MedicinaPropiaWS"
        binding="basicHttpBinding" bindingConfiguration="MedicinaPropiaWSPortBinding"
        contract="ServicioWS.MedicinaPropiaWS" name="MedicinaPropiaWSPort" />
      <endpoint address="http://localhost:8080/FarmaSoftWSBL/DetalleSolicitudWS"
        binding="basicHttpBinding" bindingConfiguration="DetalleSolicitudWSPortBinding"
        contract="ServicioWS.DetalleSolicitudWS" name="DetalleSolicitudWSPort" />
      <endpoint address="http://localhost:8080/FarmaSoftWSBL/AdministradorWS"
        binding="basicHttpBinding" bindingConfiguration="AdministradorWSPortBinding"
        contract="ServicioWS.AdministradorWS" name="AdministradorWSPort" />
    </client>
  </system.serviceModel>
</configuration>